pipeline {
    agent any

    stages {
        stage('Hello') {
            steps {
                echo 'Hello World'
            }
        }
        stage('Install Dependencies') {
            steps {
                sh 'pip3 install pytest'
            }
        }
        stage('') {
            steps {
                sh 'pytest code1.py -v'
            }
        }
    }
}
